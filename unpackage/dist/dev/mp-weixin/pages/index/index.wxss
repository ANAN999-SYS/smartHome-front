.container {
	background: linear-gradient(to bottom, #42a6ef, #2f80ed);
	align-items: stretch;
	padding: 0;
	height: 100%;
	overflow: hidden;
}
.content {
	flex: 1;
	display: flex;
	z-index: 2;
	flex-direction: column;
	align-items: stretch;
	justify-content: center;
	position: relative;
	width: 100%;
	height: 100%;
	padding-bottom: 480rpx;
	background: -webkit-gradient(linear, left top, left bottom, from(rgba(244, 244, 244, 0)), color-stop(0.1, #f4f4f4), to(#f4f4f4));
	opacity: 0;
	-webkit-transform: translate3d(0, 100%, 0);
	        transform: translate3d(0, 100%, 0);
	-webkit-animation: rise 3s cubic-bezier(0.19, 1, 0.22, 1) .25s forwards;
	        animation: rise 3s cubic-bezier(0.19, 1, 0.22, 1) .25s forwards;
}
@-webkit-keyframes rise {
0% {
		opacity: 0;
		-webkit-transform: translate3d(0, 100%, 0);
		        transform: translate3d(0, 100%, 0);
}
50% {
		opacity: 1;
}
100% {
		opacity: 1;
		-webkit-transform: translate3d(0, 450rpx, 0);
		        transform: translate3d(0, 450rpx, 0);
}
}
@keyframes rise {
0% {
		opacity: 0;
		-webkit-transform: translate3d(0, 100%, 0);
		        transform: translate3d(0, 100%, 0);
}
50% {
		opacity: 1;
}
100% {
		opacity: 1;
		-webkit-transform: translate3d(0, 450rpx, 0);
		        transform: translate3d(0, 450rpx, 0);
}
}
@-webkit-keyframes show {
0% {
		opacity: 0;
}
100% {
		opacity: .95;
}
}
@keyframes show {
0% {
		opacity: 0;
}
100% {
		opacity: .95;
}
}
.hd {
	position: absolute;
	top: 15rpx;
	left: 50%;
	width: 1000rpx;
	margin-left: -500rpx;
	height: 200rpx;
	transition: all .35s ease;
}
@-webkit-keyframes sway {
0% {
		-webkit-transform: translate3d(0, 20rpx, 0) rotate(-15deg);
		        transform: translate3d(0, 20rpx, 0) rotate(-15deg);
}
17% {
		-webkit-transform: translate3d(0, 0rpx, 0) rotate(25deg);
		        transform: translate3d(0, 0rpx, 0) rotate(25deg);
}
34% {
		-webkit-transform: translate3d(0, -20rpx, 0) rotate(-20deg);
		        transform: translate3d(0, -20rpx, 0) rotate(-20deg);
}
50% {
		-webkit-transform: translate3d(0, -10rpx, 0) rotate(15deg);
		        transform: translate3d(0, -10rpx, 0) rotate(15deg);
}
67% {
		-webkit-transform: translate3d(0, 10rpx, 0) rotate(-25deg);
		        transform: translate3d(0, 10rpx, 0) rotate(-25deg);
}
84% {
		-webkit-transform: translate3d(0, 15rpx, 0) rotate(15deg);
		        transform: translate3d(0, 15rpx, 0) rotate(15deg);
}
100% {
		-webkit-transform: translate3d(0, 20rpx, 0) rotate(-15deg);
		        transform: translate3d(0, 20rpx, 0) rotate(-15deg);
}
}
@keyframes sway {
0% {
		-webkit-transform: translate3d(0, 20rpx, 0) rotate(-15deg);
		        transform: translate3d(0, 20rpx, 0) rotate(-15deg);
}
17% {
		-webkit-transform: translate3d(0, 0rpx, 0) rotate(25deg);
		        transform: translate3d(0, 0rpx, 0) rotate(25deg);
}
34% {
		-webkit-transform: translate3d(0, -20rpx, 0) rotate(-20deg);
		        transform: translate3d(0, -20rpx, 0) rotate(-20deg);
}
50% {
		-webkit-transform: translate3d(0, -10rpx, 0) rotate(15deg);
		        transform: translate3d(0, -10rpx, 0) rotate(15deg);
}
67% {
		-webkit-transform: translate3d(0, 10rpx, 0) rotate(-25deg);
		        transform: translate3d(0, 10rpx, 0) rotate(-25deg);
}
84% {
		-webkit-transform: translate3d(0, 15rpx, 0) rotate(15deg);
		        transform: translate3d(0, 15rpx, 0) rotate(15deg);
}
100% {
		-webkit-transform: translate3d(0, 20rpx, 0) rotate(-15deg);
		        transform: translate3d(0, 20rpx, 0) rotate(-15deg);
}
}
.wave {
	position: absolute;
	z-index: 2;
	right: 0;
	bottom: -3rpx;
	opacity: 1;
	height: 260rpx;
	width: 2250rpx;
	-webkit-animation: wave 10s linear infinite;
	        animation: wave 10s linear infinite;
}
.wave-bg {
	position: absolute;
	right: 0;
	bottom: 0;
	opacity: 0.725;
	height: 260rpx;
	width: 2250rpx;
	z-index: 1;
	-webkit-animation: wave-bg 10.3s linear infinite;
	        animation: wave-bg 10.3s linear infinite;
}
@-webkit-keyframes wave {
from {
		-webkit-transform: translate3d(125rpx, 0, 0);
		        transform: translate3d(125rpx, 0, 0);
}
to {
		-webkit-transform: translate3d(1125rpx, 0, 0);
		        transform: translate3d(1125rpx, 0, 0);
}
}
@keyframes wave {
from {
		-webkit-transform: translate3d(125rpx, 0, 0);
		        transform: translate3d(125rpx, 0, 0);
}
to {
		-webkit-transform: translate3d(1125rpx, 0, 0);
		        transform: translate3d(1125rpx, 0, 0);
}
}
@-webkit-keyframes wave-bg {
from {
		-webkit-transform: translate3d(375rpx, 0, 0);
		        transform: translate3d(375rpx, 0, 0);
}
to {
		-webkit-transform: translate3d(1375rpx, 0, 0);
		        transform: translate3d(1375rpx, 0, 0);
}
}
@keyframes wave-bg {
from {
		-webkit-transform: translate3d(375rpx, 0, 0);
		        transform: translate3d(375rpx, 0, 0);
}
to {
		-webkit-transform: translate3d(1375rpx, 0, 0);
		        transform: translate3d(1375rpx, 0, 0);
}
}
.bd {
	position: relative;
	flex: 1;
	display: flex;
	flex-direction: column;
	align-items: stretch;
	-webkit-animation: bd-rise 2s cubic-bezier(0.23, 1, 0.32, 1) .75s forwards;
	        animation: bd-rise 2s cubic-bezier(0.23, 1, 0.32, 1) .75s forwards;
	opacity: 0;
}
@-webkit-keyframes bd-rise {
from {
		opacity: 0;
		-webkit-transform: translate3d(0, 60rpx, 0);
		        transform: translate3d(0, 60rpx, 0);
}
to {
		opacity: 1;
		-webkit-transform: translate3d(0, 0, 0);
		        transform: translate3d(0, 0, 0);
}
}
@keyframes bd-rise {
from {
		opacity: 0;
		-webkit-transform: translate3d(0, 60rpx, 0);
		        transform: translate3d(0, 60rpx, 0);
}
to {
		opacity: 1;
		-webkit-transform: translate3d(0, 0, 0);
		        transform: translate3d(0, 0, 0);
}
}

