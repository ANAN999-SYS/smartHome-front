
page {
	background: #f6f6f6;
}
.roomList {
	margin-top: 20rpx;
	margin-left: 20rpx;
	margin-right: 20rpx;
}
.roomCard {
	margin-bottom: 20rpx;
}
.btn-wrapper {
	height: 100%;
}
.btn {
	display: inline-flex;
	justify-content: center;
	align-items: center;
	width: 120rpx;
	height: 100%;
	color: white;
}
.delete-btn {
	background-color: #e34d59;
}
.edit-btn {
	background-color: #ed7b2f;
}
.favor-btn {
	background-color: #0052d9;
}
.column {
	flex-direction: column;
}
.padding-bottom {
	padding-bottom: 8rpx;
}
.block {
	width: 80vw;
	height: 240px;
	background: #fff;
	border-radius: 16rpx;
	position: relative;
}
.popHeard {
	text-align: center;
	font-size: 36rpx;
	margin-top: 20rpx;
}
.inputBox {
	height: 60%;
	display: flex;
	align-items: center;
	margin-left: 20rpx;
	margin-right: 20rpx;
}
.btnBox {
	position: absolute;
	bottom: 10rpx;
	display: flex;
	justify-content: center;
	width: 100%;
}
.cardBoxbtn {
	background: #00aaff !important;
	--td-button-primary-border-color: #00aaff;
}
.noneData {
	display: flex;
	margin-top: 20rpx;
	margin-left: 20rpx;
	margin-right: 20rpx;
	height: 300rpx;
	align-items: center;
	justify-content: center;
	background: #fff;
	border-radius: 16rpx;
}

